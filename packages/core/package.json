{
  "name": "@stableview/core",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.js"
    },
    "./client": {
      "import": "./dist/client/index.js",
      "types": "./dist/client/index.js"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "types": "./dist/server/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/server/api.js",
    "start:api": "node dist/server/api.js",
    "dev:api": "tsx src/server/api.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json}\"",
    "type-check": "tsc --noEmit",
    "update-metrics": "node dist/src/scripts/update-metrics.js",
    "update-metrics:dev": "tsx src/scripts/update-metrics.ts",
    "update-prices": "node dist/src/scripts/update-prices.js",
    "update-prices:dev": "tsx src/scripts/update-prices.ts",
    "update-peg-prices": "node dist/src/scripts/update-peg-prices.js",
    "update-peg-prices:dev": "tsx src/scripts/update-peg-prices.ts",
    "vercel-build": "tsc",
    "clean": "rm -rf dist",
    "db:generate": "echo 'No database generation script defined'",
    "db:push": "echo 'No database push script defined'"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@supabase/supabase-js": "^2.39.0",
    "@trpc/client": "^11.0.0-next-beta.294",
    "@trpc/server": "^11.0.0-next-beta.294",
    "@vercel/node": "^3.0.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.1",
    "superjson": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "eslint": "^8.56.0",
    "next": "^15.4.5",
    "prettier": "^3.2.5",
    "tsx": "^4.20.4",
    "typescript": "^5"
  }
}
