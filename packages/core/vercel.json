{
  "buildCommand": "yarn build",
  "outputDirectory": "dist",
  "framework": null,
  "installCommand": "yarn install",
  "devCommand": "yarn dev:api",
  "functions": {
    "api/[...slug].ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/[...slug]"
    },
    {
      "source": "/health",
      "destination": "/api/[...slug]"
    }
  ]
}
